{
  "name": "dashboard-ui",
  "version": "0.0.1",
  "private": true,
  "description": "Next.js UI fÃ¼r Dashboard-Management",
  "browserslist": [
    "chrome 111",
    "edge 111",
    "firefox 111",
    "safari 16.4"
  ],
  "preview": {
    "headerTitel": "Dashboard",
    "repoUrl": "https://github.com/munirmardinli/dashboard"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack 2>&1 | grep -vE '(Warning.*localstorage-file|warn.*Failed to get the last modified timestamp|Use.*node --trace-warnings)' || true",
    "postbuild": "pagefind --site .next/server/app --output-path out/_pagefind && cp out/_not-found/index.html out/404.html 2>/dev/null || true",
    "start": "npx serve@latest out",
    "lint": "echo 'No linting configured'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zustand": "^5.0.10",
    "nextra": "4.6.1",
    "nextra-theme-docs": "4.6.1",
    "katex": "0.16.27"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@next/bundle-analyzer": "16.1.1",
    "typescript": "^5",
    "pagefind": "^1.4.0"
  }
}
