{
  "name": "dashboard-ui",
  "version": "0.0.1",
  "private": true,
  "description": "Next.js UI fÃ¼r Dashboard-Management",
  "browserslist": [
    "chrome 111",
    "edge 111",
    "firefox 111",
    "safari 16.4"
  ],
  "preview": {
    "headerTitel": "Dashboard",
    "repoUrl": "https://github.com/munirmardinli/dashboard"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack 2>&1 | grep -vE '(Warning.*localstorage-file|warn.*Failed to get the last modified timestamp|Use.*node --trace-warnings)' || true",
    "postbuild": "pagefind --site .next/server/app --output-path out/_pagefind && cp out/_not-found/index.html out/404.html 2>/dev/null || true",
    "start": "npx serve@latest out",
    "lint": "echo 'No linting configured'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "katex": "0.16.21",
    "lucide-react": "0.563.0",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "10.1.0",
    "rehype-katex": "7.0.0",
    "rehype-raw": "7.0.0",
    "remark-gfm": "4.0.1",
    "remark-math": "6.0.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "pagefind": "^1.4.0",
    "typescript": "^5"
  }
}
