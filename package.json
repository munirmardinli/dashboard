{
  "name": "dashboard-root",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@11.7.0",
  "description": "Full-stack Management Dashboard for todos, finances, and contacts. Built with Next.js 15, React 19, TypeScript, and modern glassmorphism design.",
  "keywords": [
    "dashboard",
    "management",
    "todo",
    "productivity",
    "multi-language"
  ],
  "author": {
    "name": "Munir Mardinli",
    "url": "https://github.com/munirmardinli"
  },
  "contributors": [
    {
      "name": "Munir Mardinli",
      "url": "https://github.com/munirmardinli"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/munirmardinli/dashboard#readme",
  "bugs": {
    "url": "https://github.com/munirmardinli/dashboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/munirmardinli/dashboard.git"
  },
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "prehomelab": "npm install",
    "homelab": "run-script-os",
    "homelab:darwin": "npm run homelab:darwin --workspace=@munirmardinli/homelab",
    "homelab:linux": "npm run homelab:linux --workspace=@munirmardinli/homelab",
    "homelab:win32": "npm run homelab:windows --workspace=@munirmardinli/homelab"
  },
  "devDependencies": {
    "run-script-os": "1.1.6",
    "turbo": "2.8.9",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
